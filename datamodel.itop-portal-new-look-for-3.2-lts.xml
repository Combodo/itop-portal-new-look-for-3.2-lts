        <brick id="badge" xsi:type="Combodo\iTop\Portal\Brick\ManageBrick" _delta="define">
        <class>UserRequest</class>
        <rank><default>64</default></rank>
        <grouping>
          <tabs>
            <show_others>true</show_others>
            <groups>
              <group id="assigned">
                <title>Assigned</title>
                <condition><![CDATA[SELECT UserRequest WHERE status="assigned"]]></condition>
                <rank>1</rank>
              </group>
              <group id="new">
                <title>New</title>
                <condition><![CDATA[SELECT UserRequest WHERE status="new"]]></condition>
                <rank>2</rank>
              </group>
            </groups>
          </tabs>
        </grouping>
        <display_modes>
          <tile><default>badge</default></tile>
        </display_modes>
        <title>
        My opened user requests by status
        </title>
        <visible>
          <navigation_menu>false</navigation_menu>
        </visible>
        <description>Here are your opened requests by status</description>
        <visible>
          <navigation_menu>false</navigation_menu>
        </visible>
        <decoration_class>
          <default>fas fa-2x fa-tag</default>
        </decoration_class>
        <opening_mode>edit</opening_mode>
      </brick>
